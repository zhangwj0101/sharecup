<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<!-- é¦–é¡µ -->
	
	<package name="index" namespace="/index" extends="default">
		<action name="pageIndex" class="com.share.action.PageIndexAction">
			<result name="success">/index.jsp</result>
		</action>
		<action name="pageIndex_*" class="com.share.action.PageIndexAction" method="{1}">
			<result name="{1}">/index/pages/{1}.jsp</result>
			<result name="region">/WEB-INF/pages/usercenter/region.jsp</result>
		</action>
	</package>
</struts>
